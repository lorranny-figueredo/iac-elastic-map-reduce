# Variáveis de configuração

# Região
region = "us-east-2"

# Projeto
project = "iac-emr"

# Owner
owner = "Lorranny"

# Ambiente
environment = "EMR"

# Grupo de serviços
emr_release_label = "emr-7.0.0"

# Instância do master
emr_main_instance_type = "m5.xlarge"

# Instância dos workers
emr_core_instance_type = "m5.xlarge"

# Número de workers
emr_core_instance_count = "2"
